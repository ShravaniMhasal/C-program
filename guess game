#include<stdio.h>
#include<stdlib.h>
int main()
{
    int number, guess;
    number=rand() %100+1;
    printf("I have chosen a number between 1 and 100.\n");
    do {
        printf("Enter your guess");
        scanf("%d",&guess);
        if(guess>number)
        {
            printf("To high! Try again.\n");
        }
        else if(guess<number)
        {
            printf("Too low! Try again.\n");
        }
        else {
        printf("correct! You guessed the number.\n");
        }
    } while (number != guess);
        return 0;
    }
